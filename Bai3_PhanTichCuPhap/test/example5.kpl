Program Example5; (* Factorial *)
   
Var n : Integer;
    i : char;

Function F(n : Integer) : Integer;
  Begin
    If n = 0 Then F := 1 Else F := N * F (N - 1);
    If n = 0 return F := 1 Else return F := 0
  End;

Begin
  For n := 1 To 7 Do
    Begin
      Call WriteLn;
      Call WriteI( F(i));
    End;
End. (* Factorial *)